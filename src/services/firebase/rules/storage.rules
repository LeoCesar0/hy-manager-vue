rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    
    // User files - only authenticated users can access their own files
    match /env/{environment}/users/{userId}/{allPaths=**} {
      allow read, write: if request.auth != null && request.auth.uid == userId;
    }
    
    // Deny everything else
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
}